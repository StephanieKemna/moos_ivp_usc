//--------------------------------------------------
// pSelectFormation configuration block
//
ProcessConfig = pSelectFormation
{
  AppTick   = 2
  CommsTick = 2
  
  // who to follow
  lead_vehicle_name = $(LEAD_NAME)
  lead_sensor_range = $(LEAD_SENSOR_RANGE)
  // what distance to follow at
  follow_range = 30
  // what distance the vehicles should keep apart in the formation
  inter_vehicle_distance = $(IVD)
}
